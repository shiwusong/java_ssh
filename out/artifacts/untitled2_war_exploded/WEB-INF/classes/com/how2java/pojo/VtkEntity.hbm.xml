<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.how2java.pojo.VtkEntity" table="vtk" schema="patient">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="path">
            <column name="path" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>